{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "VERCEL_START_COMMAND": "python app/main.py"
  },
  "functions": {
    "api/**": {
      "compression": "gzip"
    }
  }
}
